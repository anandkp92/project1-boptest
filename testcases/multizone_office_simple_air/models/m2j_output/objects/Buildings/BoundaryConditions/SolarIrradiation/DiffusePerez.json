      "type": "long_class_specifier",
      "annotation": [
        {
          "element_modification_or_replaceable": {
            "element_modification": {
              "name": "defaultComponentName",
              "modification": {
                "equal": true,
                "expression": {
                  "simple_expression": "\"HDifTil\""
