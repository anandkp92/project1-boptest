        "declaration": {
          "identifier": "k",
          "array_subscripts": [
            {
              "expression": {
                "simple_expression": "n"
              }
            }
          ],
          "modification": {
            "class_modification": [
              {
                "element_modification_or_replaceable": {
                  "each": true,
                  "element_modification": {
                    "name": "unit",
                    "modification": {
                      "equal": true,
                      "expression": {
                        "simple_expression": "\"W/K4\""
                      }
                    }
                  }
                }
              }
            ],
            "equal": true,
            "expression": {
              "simple_expression": {
                "for_loop": {
                  "expression": {
                    "simple_expression": "4*A[i]*Modelica.Constants.sigma*absIR[i]"
                  },
                  "for_loop": [
                    {
                      "name": "i",
                      "range": "1:n"
                    }
                  ]
                }
              }
            }
          }
        },
        "description": {
          "description_string": "Constant for radiative heat exchange"
        }
