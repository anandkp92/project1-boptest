      "annotation": [
        {
          "element_modification_or_replaceable": {
            "element_modification": {
              "name": "Documentation",
              "modification": {
                "class_modification": [
                  {
                    "element_modification_or_replaceable": {
                      "element_modification": {
                        "name": "info",
                        "modification": {
                          "equal": true,
                          "expression": {
                            "simple_expression": "\"<html>\n<p>\nThis model computes the radiant, convective sensible and latent heat flow rate.\nInput into this model are these three components in units of [W/m2].\nThe inputs need to be positive quantities if heat or moisture is added\nto the room.\nThe outputs are\n</p>\n<ul>\n<li>\nthe radiant heat flow in Watts,\n</li>\n<li>\nthe convective heat flow in Watts, and\n</li>\n<li>\nthe water vapor released into the air.\n</li>\n</ul>\n</html>\""
                          }
                        }
                      }
                    }
                  },
                  {
                    "element_modification_or_replaceable": {
                      "element_modification": {
                        "name": "revisions",
                        "modification": {
                          "equal": true,
                          "expression": {
                            "simple_expression": "\"<html>\n<ul>\n<li>\nOctober 25, 2021, by Michael Wetter:<br/>\nUpdated documentation to improve clarity.\n</li>\n<li>\nMay 2, 2016, by Michael Wetter:<br/>\nRefactored implementation of latent heat gain.\nThis is for\n<a href=\\\"https://github.com/lbl-srg/modelica-buildings/issues/515\\\">issue 515</a>.\n</li>\n<li>\nAugust 1, 2013, by Michael Wetter:<br/>\nFirst implementation.\n</li>\n</ul>\n</html>\""
                          }
                        }
                      }
                    }
                  }
                ]
              }
            }
          }
