              {
                "equation_section": {
                  "equation": [
                    {
                      "assignment_equation": {
                        "lhs": "0",
                        "rhs": {
                          "simple_expression": "port_a.Q_flow +port_b.Q_flow +QAbs_flow +JIn_a +JIn_b -JOut_a -JOut_b"
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "u*(port_b.T -port_a.T)",
                        "rhs": {
                          "simple_expression": "2*R*(-port_a.Q_flow -QAbs_flow/2 -JIn_a +JOut_a +tauIR*(JIn_a -JIn_b))"
                        }
                      }
                    },
                    {
                      "if_equation": {
                        "if_elseif": [
                          {
                            "condition": {
                              "simple_expression": "linearize"
                            },
                            "then": [
                              {
                                "equation": {
                                  "assignment_equation": {
                                    "lhs": "T4_a",
                                    "rhs": {
                                      "simple_expression": "4*T03*port_a.T -3*T04"
                                    }
                                  }
                                }
                              },
                              {
                                "equation": {
                                  "assignment_equation": {
                                    "lhs": "T4_b",
                                    "rhs": {
                                      "simple_expression": "4*T03*port_b.T -3*T04"
                                    }
                                  }
                                }
                              }
                            ]
                          }
                        ],
                        "else_equation": [
                          {
                            "if_equation": {
                              "if_elseif": [
                                {
                                  "condition": {
                                    "simple_expression": "homotopyInitialization"
                                  },
                                  "then": [
                                    {
                                      "equation": {
                                        "assignment_equation": {
                                          "lhs": "T4_a",
                                          "rhs": {
                                            "simple_expression": {
                                              "function_call": {
                                                "name": "homotopy",
                                                "arguments": [
                                                  {
                                                    "name": "actual=port_a.T^4,simplified=4*T03*port_a.T -3*T04"
                                                  }
                                                ]
                                              }
                                            }
                                          }
                                        }
                                      }
                                    },
                                    {
                                      "equation": {
                                        "assignment_equation": {
                                          "lhs": "T4_b",
                                          "rhs": {
                                            "simple_expression": {
                                              "function_call": {
                                                "name": "homotopy",
                                                "arguments": [
                                                  {
                                                    "name": "actual=port_b.T^4,simplified=4*T03*port_b.T -3*T04"
                                                  }
                                                ]
                                              }
                                            }
                                          }
                                        }
                                      }
                                    }
                                  ]
                                }
                              ],
                              "else_equation": [
                                {
                                  "assignment_equation": {
                                    "lhs": "T4_a",
                                    "rhs": {
                                      "simple_expression": "port_a.T^4"
                                    }
                                  }
                                },
                                {
                                  "assignment_equation": {
                                    "lhs": "T4_b",
                                    "rhs": {
                                      "simple_expression": "port_b.T^4"
                                    }
                                  }
                                }
                              ]
                            }
                          }
                        ]
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "E_a",
                        "rhs": {
                          "simple_expression": "u*A*absIR_a*Modelica.Constants.sigma*T4_a"
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "E_b",
                        "rhs": {
                          "simple_expression": "u*A*absIR_b*Modelica.Constants.sigma*T4_b"
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "JOut_a",
                        "rhs": {
                          "simple_expression": "E_a +rhoIR_a*JIn_a +tauIR*JIn_b"
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "JOut_b",
                        "rhs": {
                          "simple_expression": "E_b +rhoIR_b*JIn_b +tauIR*JIn_a"
                        }
                      }
                    }
                  ]
                }
              }
