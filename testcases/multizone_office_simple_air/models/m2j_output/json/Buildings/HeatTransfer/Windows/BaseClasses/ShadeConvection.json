            "element_sections": [
              {
                "equation_section": {
                  "equation": [
                    {
                      "if_equation": {
                        "if_elseif": [
                          {
                            "condition": {
                              "simple_expression": "thisSideHasShade"
                            },
                            "then": [
                              {
                                "equation": {
                                  "assignment_equation": {
                                    "lhs": "air.Q_flow",
                                    "rhs": {
                                      "simple_expression": "Gc*(2*(air.T -TSha) +(air.T -glass.T))"
                                    }
                                  }
                                }
                              },
                              {
                                "equation": {
                                  "assignment_equation": {
                                    "lhs": "glass.Q_flow",
                                    "rhs": {
                                      "simple_expression": "Gc*((glass.T -air.T) +k*(glass.T -TSha))"
                                    }
                                  }
                                }
                              },
                              {
                                "equation": {
                                  "assignment_equation": {
                                    "lhs": "air.Q_flow +glass.Q_flow +QRadAbs_flow",
                                    "rhs": {
                                      "simple_expression": "0"
                                    }
                                  }
                                }
                              }
                            ]
                          }
                        ],
                        "else_equation": [
                          {
                            "assignment_equation": {
                              "lhs": "air.Q_flow",
                              "rhs": {
                                "simple_expression": "Gc*(air.T -glass.T)"
                              }
                            }
                          },
                          {
                            "assignment_equation": {
                              "lhs": "air.Q_flow +glass.Q_flow",
                              "rhs": {
                                "simple_expression": "0"
                              }
                            }
                          },
                          {
                            "assignment_equation": {
                              "lhs": "TSha",
                              "rhs": {
                                "simple_expression": "(air.T +glass.T)/2"
                              }
                            }
                          }
                        ]
                      }
                    }
                  ]
