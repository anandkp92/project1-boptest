                  },
                  {
                    "final": true,
                    "component_clause": {
                      "type_prefix": "parameter",
                      "type_specifier": "Real",
                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "coeTraWinExtIrr",
                            "array_subscripts": [
                              {
                                "expression": {
                                  "simple_expression": "2"
                                }
                              },
                              {
                                "expression": {
                                  "simple_expression": "radDat.HEM +2"
                                }
                              },
                              {
                                "expression": {
                                  "simple_expression": "NSta"
                                }
                              }
                            ],
                            "modification": {
                              "class_modification": [
                                {
                                  "element_modification_or_replaceable": {
                                    "each": true,
                                    "element_modification": {
                                      "name": "fixed",
                                      "modification": {
                                        "equal": true,
                                        "expression": {
                                          "simple_expression": "false"
                                        }
                                      }
                                    }
                                  }
                                }
                              ]
                            }
                          }
                        }
                      ]
                    }
                  },
                  {
                    "component_clause": {
                      "type_specifier": "Real",
                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "incAng2"
                          },
                          "description": {
                            "description_string": "=min(incAng, 0.5*Modelica.Constants.pi)"
                          }
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "equation_section": {
                  "initial": true,
                  "equation": [
                    {
                      "for_equation": {
                        "for_indices": [
                          {
                            "identifier": "j",
                            "expression": {
                              "simple_expression": "1:HEM"
                            }
                          }
                        ],
                        "loop_equations": [
                          {
                            "assignment_equation": {
                              "lhs": "coeTraWinExtIrr[NoShade,j +1,1:NSta]",
                              "rhs": {
                                "simple_expression": "radDat.traRef[1,1,N,j,1:NSta]"
                              }
                            }
                          },
                          {
                            "if_equation": {
                              "if_elseif": [
                                {
                                  "condition": {
                                    "simple_expression": "haveInteriorShade"
                                  },
                                  "then": [
                                    {
                                      "equation": {
                                        "assignment_equation": {
                                          "lhs": "coeTraWinExtIrr[Shade,j +1,1:NSta]",
                                          "rhs": {
                                            "simple_expression": "radDat.winTraExtIrrIntSha[j,1:NSta]"
                                          }
                                        }
                                      }
                                    }
                                  ]
                                },
                                {
                                  "condition": {
                                    "simple_expression": "haveExteriorShade"
                                  },
                                  "then": [
                                    {
                                      "equation": {
                                        "assignment_equation": {
                                          "lhs": "coeTraWinExtIrr[Shade,j +1,1:NSta]",
                                          "rhs": {
                                            "simple_expression": "radDat.winTraExtIrrExtSha[j,1:NSta]"
                                          }
                                        }
                                      }
                                    }
                                  ]
                                }
                              ],
                              "else_equation": [
                                {
                                  "assignment_equation": {
                                    "lhs": "coeTraWinExtIrr[Shade,j +1,1:NSta]",
                                    "rhs": {
                                      "simple_expression": {
                                        "function_call": {
                                          "name": "zeros",
                                          "arguments": [
                                            {
                                              "name": "NSta"
                                            }
                                          ]
                                        }
                                      }
                                    }
                                  }
                                }
                              ]
                            }
                          }
                        ]
                      }
                    },
                    {
                      "for_equation": {
                        "for_indices": [
                          {
                            "identifier": "k",
                            "expression": {
                              "simple_expression": "NoShade:Shade"
                            }
                          }
                        ],
                        "loop_equations": [
                          {
                            "assignment_equation": {
                              "lhs": "coeTraWinExtIrr[k,1,1:NSta]",
                              "rhs": {
                                "simple_expression": "coeTraWinExtIrr[k,2,1:NSta]"
                              }
                            }
                          },
                          {
                            "assignment_equation": {
                              "lhs": "coeTraWinExtIrr[k,HEM +2,1:NSta]",
                              "rhs": {
                                "simple_expression": "coeTraWinExtIrr[k,HEM +1,1:NSta]"
                              }
                            }
                          }
                        ]
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "traCoeRoo",
                        "rhs": {
                          "simple_expression": "radDat.traRef[1,N,1,HEM,1:NSta]"
                        }
                      }
                    }
                  ]
                }
              },
              {
                "equation_section": {
                  "equation": [
                    {
                      "assignment_equation": {
                        "lhs": "incAng2",
                        "rhs": {
                          "simple_expression": {
                            "function_call": {
                              "name": "min",
                              "arguments": [
                                {
                                  "name": "incAng"
                                },
                                {
                                  "name": "0.5*Modelica.Constants.pi"
                                }
                              ]
                            }
                          }
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "x",
                        "rhs": {
                          "simple_expression": "(2*(NDIR -1)*abs(incAng2)/Modelica.Constants.pi) +2"
                        }
                      },
                      "description": {
                        "description_string": "x=(index-1)*incAng/(0.5pi)+2, 0<=x<=NDIR-1"
                      }
                    },
                    {
                      "for_equation": {
                        "for_indices": [
                          {
                            "identifier": "iSta",
                            "expression": {
                              "simple_expression": "1:NSta"
                            }
                          }
                        ],
                        "loop_equations": [
                          {
                            "assignment_equation": {
                              "lhs": "QTraDifUns_flow[iSta]",
                              "rhs": {
                                "simple_expression": "AWin*HDif*(1 -uSha_internal)*coeTraWinExtIrr[NoShade,HEM +1,iSta]"
                              }
                            }
                          },
                          {
                            "assignment_equation": {
                              "lhs": "QTraDirUns_flow[iSta]",
                              "rhs": {
                                "simple_expression": "AWin*HDir*(1 -uSha_internal)*Buildings.HeatTransfer.Windows.BaseClasses.smoothInterpolation({coeTraWinExtIrr[NoShade,k,iSta] for k in 1:(HEM +2)},x)"
                              }
                            }
                          }
                        ]
                      }
                    },
                    {
                      "for_equation": {
                        "for_indices": [
                          {
                            "identifier": "iSta",
                            "expression": {
                              "simple_expression": "1:NSta"
                            }
                          }
                        ],
                        "loop_equations": [
                          {
                            "assignment_equation": {
                              "lhs": "QTraDifSha_flow[iSta]",
                              "rhs": {
                                "simple_expression": "AWin*HDif*uSha_internal*coeTraWinExtIrr[Shade,HEM +1,iSta]"
                              }
                            }
                          },
                          {
                            "assignment_equation": {
                              "lhs": "QTraDirSha_flow[iSta]",
                              "rhs": {
                                "simple_expression": "AWin*HDir*uSha_internal*Buildings.HeatTransfer.Windows.BaseClasses.smoothInterpolation({coeTraWinExtIrr[Shade,k,iSta] for k in 1:(HEM +2)},x)"
                              }
                            }
                          }
                        ]
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "QTraDif_flow",
                        "rhs": {
                          "simple_expression": "QTraDifUns_flow +QTraDifSha_flow"
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "QTraDir_flow",
                        "rhs": {
                          "simple_expression": "QTraDirUns_flow +QTraDirSha_flow"
                        }
                      }
                    }
                  ]
                }
              }
            ],
            "annotation": [
              {
                "element_modification_or_replaceable": {
                  "element_modification": {
                    "name": "Documentation",
                    "modification": {
                      "class_modification": [
                        {
                          "element_modification_or_replaceable": {
                            "element_modification": {
                              "name": "info",
                              "modification": {
                                "equal": true,
                                "expression": {
                                  "simple_expression": "\"<html>\n<p>\nThe model calculates the solar radiation through the window.\nThe calculations follow the description in Wetter (2004), Appendix A.4.3.\n</p>\n<p>\nThe transmitted exterior radiation for window system includes:\n</p>\n<ol>\n<li>\nthe transmitted diffusive radiation on unshaded part: <code>AWin*(1-uSha)*HDif*tau(HEM)</code>\n</li>\n<li>\nthe transmitted direct radiation on no shade part: <code>AWin*(1-uSha)*HDir*tau(IncAng)</code>\n</li>\n<li>\nthe transmitted diffusive radiation on shaded part: <code>AWin*uSha*HDif*tauSha(HEM)</code>\n</li>\n<li>\nthe transmitted direct radiation on shaded part: <code>AWin*uSha*HDir*tauSha(IncAng);</code>\n</li>\n</ol>\n<p>\nThe outputs are <code>QTraDif_flow = Part1 + Part3</code> and\n<code>QTraDir_flow = Part2 + Part4</code>.\n</p>\n<h4>References</h4>\n<ul>\n<li>\nMichael Wetter.<br/>\n<a href=\\\"http://simulationresearch.lbl.gov/wetter/download/mwdiss.pdf\\\">\nSimulation-based Building Energy Optimization</a>.<br/>\nDissertation. University of California at Berkeley. 2004.\n</li>\n</ul>\n</html>\""
                                }
                              }
                            }
                          }
                        },
                        {
                          "element_modification_or_replaceable": {
                            "element_modification": {
                              "name": "revisions",
                              "modification": {
                                "equal": true,
                                "expression": {
                                  "simple_expression": "\"<html>\n<ul>\n<li>\nJune 7, 2016, by Michael Wetter:<br/>\nRemoved output <code>QTra_flow</code> and introduced instead\n<code>QTraDif_flow</code> and <code>QTraDir_flow</code>.\nThis is for\n<a href=\\\"https://github.com/lbl-srg/modelica-buildings/issues/451\\\">issue 451</a>.\n</li>\n<li>\nAugust 7, 2015, by Michael Wetter:<br/>\nRevised model to allow modeling of electrochromic windows.\nThis is for\n<a href=\\\"https://github.com/lbl-srg/modelica-buildings/issues/445\\\">issue 445</a>.\n</li>\n<li>\nMarch 13, 2015, by Michael Wetter:<br/>\nChanged <code>initial algorithm</code>\nto <code>initial equation</code> section\nto avoid a translation error in OpenModelica.\n</li>\n<li>\nMarch 4, 2011, by Wangda Zuo:<br/>\nRemove the if-statement and integer function that can trigger events.\n</li>\n<li>\nFebruary 2, 2010, by Michael Wetter:<br/>\nMade connector <code>uSha</code> a conditional connector.\n</li>\n<li>\nDecember 15, 2010, by Wangda Zuo:<br/>\nSeparate transmittance and absorbance models from the window radiation model.\n</li>\n<li>\nDecember 12, 2010, by Michael Wetter:<br/>\nReplaced record\n<a href=\\\"modelica://Buildings.HeatTransfer.Data.GlazingSystems\\\">\nBuildings.HeatTransfer.Data.GlazingSystems</a> with the\nparameters used by this model.\nThis was needed to integrate the radiation model into the room model.\n</li>\n<li>\nDecember 10, 2010, by Wangda Zuo:<br/>\nFirst implementation.\n</li>\n</ul>\n</html>\""
                                }
                              }
                            }
                          }
                        }
                      ]
                    }
                  }
                }
              },
