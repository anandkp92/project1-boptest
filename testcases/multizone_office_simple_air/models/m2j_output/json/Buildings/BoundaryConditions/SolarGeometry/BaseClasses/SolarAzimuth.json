                              "arguments": [
                                {
                                  "name": "arg"
                                }
                              ]
                            }
                          }
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "solAzi",
                        "rhs": {
                          "if_expression": {
                            "if_elseif": [
                              {
                                "condition": {
                                  "simple_expression": "outsidePolarCircle"
                                },
                                "then": {
                                  "simple_expression": {
                                    "function_call": {
                                      "name": "solarAzimuthNoEvent",
                                      "arguments": [
                                        {
                                          "name": "solAziTem"
                                        },
                                        {
                                          "name": "solTim,day"
                                        }
                                      ]
                                    }
                                  }
                                }
                              }
                            ],
                            "else_expression": {
                              "simple_expression": {
                                "function_call": {
                                  "name": "solarAzimuthWithEvent",
                                  "arguments": [
                                    {
                                      "name": "solAziTem"
                                    },
                                    {
                                      "name": "solTim,day"
                                    }
                                  ]
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  ]
                }
