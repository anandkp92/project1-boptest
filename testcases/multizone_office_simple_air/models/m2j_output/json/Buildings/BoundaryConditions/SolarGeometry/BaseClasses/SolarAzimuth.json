                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "outsidePolarCircle",
                            "modification": {
                              "equal": true,
                              "expression": {
                                "simple_expression": {
                                  "logical_expression": {
                                    "logical_or": [
                                      {
                                        "logical_and": [
                                          {
                                            "arithmetic_expressions": [
                                              {
                                                "name": "lat"
                                              },
                                              {
                                                "name": "polarCircle"
                                              }
                                            ],
                                            "relation_operator": "<"
                                          },
                                          {
                                            "arithmetic_expressions": [
                                              {
                                                "name": "lat"
                                              },
                                              {
                                                "name": "-polarCircle"
                                              }
                                            ],
                                            "relation_operator": ">"
                                          }
                                        ]
                                      }
                                    ]
                                  }
                                }
                              }
                            }
                          },
                          "description": {
                            "description_string": "Flag, true if latitude is outside polar region"
                          }
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "equation_section": {
                  "equation": [
                    {
                      "assignment_equation": {
                        "lhs": "tmp",
                        "rhs": {
                          "simple_expression": "(Modelica.Math.sin(lat)*Modelica.Math.cos(zen) -Modelica.Math.sin(decAng))/(Modelica.Math.cos(lat)*Modelica.Math.sin(zen))"
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "arg",
                        "rhs": {
                          "simple_expression": {
                            "function_call": {
                              "name": "min",
                              "arguments": [
                                {
                                  "name": "1"
                                },
                                {
                                  "name": "max(-1,tmp)"
                                }
                              ]
                            }
                          }
                        }
                      }
                    },
                    {
                      "assignment_equation": {
