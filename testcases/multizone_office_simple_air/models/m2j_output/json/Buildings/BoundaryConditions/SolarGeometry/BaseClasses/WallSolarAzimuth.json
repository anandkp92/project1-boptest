                          "declaration": {
                            "identifier": "ninety",
                            "modification": {
                              "equal": true,
                              "expression": {
                                "simple_expression": "Modelica.Constants.pi/2 -delta"
                              }
                            }
                          },
                          "description": {
                            "description_string": "+89 degree"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "component_clause": {
                      "type_prefix": "constant",
                      "type_specifier": "Real",
                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "deltaX",
                            "modification": {
                              "equal": true,
                              "expression": {
                                "simple_expression": "0.0001"
                              }
                            }
                          },
                          "description": {
                            "description_string": "Small number used for smoothing"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "component_clause": {
                      "type_specifier": "Real",
                      "component_list": [
                        {
                          "declaration": {
