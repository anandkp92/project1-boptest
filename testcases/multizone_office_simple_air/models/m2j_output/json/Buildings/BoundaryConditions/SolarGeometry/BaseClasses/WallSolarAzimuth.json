                          },
                          "description": {
                            "description_string": "Cosine of altitude, bounded away from zero"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "component_clause": {
                      "type_specifier": "Real",
                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "rat"
                          },
                          "description": {
                            "description_string": "Ratio of cosines"
                          }
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "equation_section": {
                  "equation": [
                    {
                      "assignment_equation": {
                        "lhs": "alt_c",
                        "rhs": {
                          "simple_expression": {
                            "function_call": {
                              "name": "Modelica.Math.cos",
                              "arguments": [
                                {
                                  "name": "Buildings.Utilities.Math.Functions.smoothLimit(x=alt,l=-ninety,u=ninety,deltaX=delta)"
                                }
                              ]
                            }
                          }
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "rat",
                        "rhs": {
                          "simple_expression": "Modelica.Math.cos(incAng)/alt_c"
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "verAzi",
                        "rhs": {
                          "simple_expression": {
                            "function_call": {
                              "name": "Modelica.Math.acos",
                              "arguments": [
                                {
                                  "name": "Buildings.Utilities.Math.Functions.smoothLimit(x=rat,l=-1 +deltaX,u=1 -deltaX,deltaX=deltaX/10)"
                                }
                              ]
                            }
                          }
                        }
                      }
                    }
                  ]
                }
