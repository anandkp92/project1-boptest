                        "lhs": "incAng",
                        "rhs": {
                          "simple_expression": {
                            "function_call": {
                              "name": "Modelica.Math.acos",
                              "arguments": [
                                {
                                  "name": "Modelica.Math.cos(til)*(dec_c*sol_c*lat_c +dec_s*lat_s) +Modelica.Math.sin(til)*(Modelica.Math.sin(azi)*dec_c*sol_s +Modelica.Math.cos(azi)*(dec_c*sol_c*lat_s -dec_s*lat_c))"
                                }
                              ]
                            }
                          }
                        }
                      },
                      "description": {
