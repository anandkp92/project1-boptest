              },
              {
                "equation_section": {
                  "equation": [
                    {
                      "assignment_equation": {
                        "lhs": "zenLim",
                        "rhs": {
                          "simple_expression": {
                            "function_call": {
                              "name": "Buildings.Utilities.Math.Functions.smoothMin",
                              "arguments": [
                                {
                                  "name": "zen"
                                },
                                {
                                  "name": "Modelica.Constants.pi/2,0.01"
                                }
                              ]
                            }
                          }
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "zenDeg",
                        "rhs": {
                          "simple_expression": "zenLim*180/Modelica.Constants.pi"
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "relAirMas",
                        "rhs": {
                          "simple_expression": "exp(-0.0001184*alt)/(Modelica.Math.cos(zenLim) +0.15*(93.9 -zenDeg)^(-1.253))"
                        }
                      }
                    }
                  ]
                }
              }
