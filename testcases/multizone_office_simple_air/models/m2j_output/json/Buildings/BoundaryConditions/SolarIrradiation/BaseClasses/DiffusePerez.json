                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "bMin",
                            "modification": {
                              "equal": true,
                              "expression": {
                                "simple_expression": {
                                  "function_call": {
                                    "name": "Modelica.Math.cos",
                                    "arguments": [
                                      {
                                        "name": "Modelica.Constants.pi*85/180"
                                      }
                                    ]
                                  }
                                }
                              }
                            }
                          },
                          "description": {
                            "description_string": "Lower bound for b"
                          }
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "equation_section": {
                  "equation": [
                    {
                      "assignment_equation": {
                        "lhs": "a",
                        "rhs": {
                          "simple_expression": {
                            "function_call": {
                              "name": "Buildings.Utilities.Math.Functions.smoothMax",
                              "arguments": [
                                {
                                  "name": "0"
                                },
                                {
                                  "name": "Modelica.Math.cos(incAng),0.01"
                                }
                              ]
                            }
                          }
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "b",
                        "rhs": {
                          "simple_expression": {
                            "function_call": {
                              "name": "Buildings.Utilities.Math.Functions.smoothMax",
                              "arguments": [
                                {
                                  "name": "bMin"
                                },
                                {
                                  "name": "Modelica.Math.cos(zen),0.01"
                                }
                              ]
                            }
                          }
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "HSkyDifTil",
                        "rhs": {
                          "simple_expression": "HDifHor*(0.5*(1 -briCof1)*(1 +Modelica.Math.cos(til)) +briCof1*a/b +briCof2*Modelica.Math.sin(til))"
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "HGroDifTil",
                        "rhs": {
                          "simple_expression": "HGloHor*0.5*rho*(1 -Modelica.Math.cos(til))"
                        }
                      }
                    }
                  ]
                }
              }
