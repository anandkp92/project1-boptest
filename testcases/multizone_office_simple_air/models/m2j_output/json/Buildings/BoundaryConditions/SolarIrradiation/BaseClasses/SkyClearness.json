            "element_sections": [
              {
                "protected_element_list": [
                  {
                    "component_clause": {
                      "type_prefix": "constant",
                      "type_specifier": "Modelica.Units.SI.Irradiance",
                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "hSmall",
                            "modification": {
                              "equal": true,
                              "expression": {
                                "simple_expression": "0.00005"
                              }
                            }
                          },
                          "description": {
                            "description_string": "Small radiation for regularization"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "component_clause": {
                      "type_prefix": "constant",
                      "type_specifier": "Modelica.Units.SI.Irradiance",
                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "deltaX",
                            "modification": {
                              "equal": true,
                              "expression": {
                                "simple_expression": "hSmall/2"
                              }
                            }
                          },
                          "description": {
                            "description_string": "Small radiation for regularization"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "component_clause": {
                      "type_prefix": "constant",
                      "type_specifier": "Real",
                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "k",
                            "modification": {
                              "equal": true,
                              "expression": {
                                "simple_expression": "0.000005534*(180/Modelica.Constants.pi)^3"
                              }
                            }
                          },
                          "description": {
                            "description_string": "Constant factor"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "component_clause": {
                      "type_specifier": "Real",
                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "tmp1"
                          },
                          "description": {
                            "description_string": "Intermediate variable"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "component_clause": {
                      "type_specifier": "Modelica.Units.SI.Irradiance",
                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "HDifHorBou"
                          },
                          "description": {
                            "description_string": "Diffuse horizontal irradiation, bounded away from zero"
                          }
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "equation_section": {
                  "equation": [
                    {
                      "assignment_equation": {
                        "lhs": "tmp1",
                        "rhs": {
                          "simple_expression": "k*zen^3"
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "HDifHorBou",
                        "rhs": {
                          "simple_expression": {
                            "function_call": {
                              "name": "Buildings.Utilities.Math.Functions.smoothMax",
                              "arguments": [
                                {
                                  "name": "x1=HDifHor,x2=hSmall,deltaX=deltaX"
                                }
                              ]
                            }
                          }
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "skyCle",
                        "rhs": {
                          "simple_expression": {
                            "function_call": {
                              "name": "Buildings.Utilities.Math.Functions.smoothLimit",
                              "arguments": [
                                {
                                  "name": "x=((HDirNor +HDifHorBou)/HDifHorBou +tmp1)/(1 +tmp1),l=1,u=8,deltaX=0.01"
                                }
                              ]
                            }
                          }
                        }
                      }
                    }
                  ]
                }
              }
